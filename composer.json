{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "*",
        "arandilopez/laravel-profane": "^0.5.0",
        "askedio/laravel5-profanity-filter": "^1.10",
        "cebe/markdown": "^1.2",
        "cjmellor/level-up": "^1.4",
        "cocur/slugify": "^4.6",
        "dg/rss-php": "^1.5",
        "doctrine/dbal": "^3.9",
        "erusev/parsedown": "^1.7",
        "ezyang/htmlpurifier": "^4.18",
        "fahlisaputra/laravel-minify": "^1.1",
        "guzzlehttp/guzzle": "^7.9",
        "http-interop/http-factory-guzzle": "^1.2",
        "illuminate/pagination": "*",
        "inertiajs/inertia-laravel": "^2.0",
        "jbbcode/jbbcode": "^1.4",
        "kornrunner/keccak": "^1.1",
        "laravel-lang/lang": "^14.7",
        "laravel/cashier": "^15.6",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.6",
        "laravel/passport": "^12.4",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.12",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.10",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/html-to-markdown": "^5.1",
        "martinbean/socialite-discord-provider": "^1.2",
        "meilisearch/meilisearch-php": "^1.12",
        "mews/purifier": "^3.4",
        "monicahq/laravel-cloudflare": "^3.7",
        "nyholm/psr7": "^1.8",
        "php-open-source-saver/jwt-auth": "^2.7",
        "phpmailer/phpmailer": "^6.9",
        "predis/predis": "^2.3",
        "pusher/pusher-php-server": "^7.2",
        "railsware/mailtrap-php": "^3.0",
        "simplito/elliptic-php": "^1.0",
        "socialiteproviders/discord": "^4.2",
        "socialiteproviders/google": "*",
        "spatie/laravel-discord-alerts": "^1.8",
        "spatie/laravel-json-api-paginate": "^1.16",
        "spatie/laravel-responsecache": "^7.6",
        "spatie/laravel-varnish": "^2.10",
        "stevebauman/location": "^7.4",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.2",
        "symfony/process": "^7.2",
        "tightenco/ziggy": "^2.5",
        "tremby/laravel-git-version": "^1.2",
        "wotz/laravel-swagger-ui": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.24",
        "laravel/breeze": "^2.3",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.40",
        "laravel/telescope": "^5.3",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/MainHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
